{
  "cli": {
    "version": ">= 5.0.0"
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "myajiri@gmail.com",
        "ascAppId": "6759110209",
        "appleTeamId": "8L93Q84K2R"
      },
      "android": {
        "serviceAccountKeyPath": "./google-play-service-account.json",
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ENABLE_PURCHASES": "true"
      }
    },
    "testing": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ENABLE_PURCHASES": "false",
        "EXPO_PUBLIC_FORCE_PREMIUM": "true"
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_ENABLE_PURCHASES": "true"
      }
    }
  }
}
